import java.util.*;

public class test{

    public static void main(String[] args) {
        
        test test = new test();
        int input = Integer.parseInt(args[0]);

        System.out.println("Summation of " + input + " is " + test.sum(input));
    }

	public int sum (int input){
	    int rsum = 0;
	    	for(int i = 0; i <= input; i++){
                rsum += i;
            }
        return rsum;
	}

}
